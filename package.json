{
  "name": "storage-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "copy-static-assets": "cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "start": "npm run build && npm run copy-static-assets && node .next/standalone/server.js",
    "lint": "next lint",
    "export": "next export",
    "predeploy": "npm run build && npm run export",
    "deploy": "gh-pages -d out"
  },
  "homepage": "https://salamoxso.github.io/storage-website",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "lucide-react": "^0.456.0",
    "next": "^15.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
